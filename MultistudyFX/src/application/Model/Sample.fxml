<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<BorderPane prefHeight="790.0" prefWidth="949.0" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.SampleController">
   <center>
      <Pane prefHeight="786.0" prefWidth="940.0" style="-fx-background-color: grey;" BorderPane.alignment="CENTER">
         <children>
            <TextArea fx:id="note" layoutX="94.0" layoutY="239.0" prefHeight="321.0" prefWidth="278.0" promptText="Add Notes..." />
            <Text layoutX="395.0" layoutY="112.0" strokeType="OUTSIDE" strokeWidth="0.0" text="NOTEPAD">
               <font>
                  <Font size="50.0" />
               </font>
            </Text>
            <Button fx:id="add" layoutX="190.0" layoutY="589.0" mnemonicParsing="false" onAction="#addNote" text="ADD NOTES" />
            <ListView fx:id="notes" layoutX="628.0" layoutY="242.0" prefHeight="321.0" prefWidth="278.0" />
            <Text layoutX="628.0" layoutY="215.0" strokeType="OUTSIDE" strokeWidth="0.0" text="NOTES FOR:">
               <font>
                  <Font size="20.0" />
               </font>
            </Text>
            <Text fx:id="date" layoutX="751.0" layoutY="213.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Text">
               <font>
                  <Font size="19.0" />
               </font>
            </Text>
         </children>
      </Pane>
   </center>
</BorderPane>
